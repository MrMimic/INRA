  Author                   = {Mayer, Klaus F. X. and Rogers, Jane and Doležel, Jaroslav and Pozniak, Curtis and Eversole, Kellye and Feuillet, Catherine and Gill, Bikram and Friebe, Bernd and Lukaszewski, Adam J. and Sourdille, Pierre and Endo, Takashi R. and Kubaláková, Marie and Číhalíková, Jarmila and Dubská, Zdeňka and Vrána, Jan and Šperková, Romana and Šimková, Hana and Febrer, Melanie and Clissold, Leah and McLay, Kirsten and Singh, Kuldeep and Chhuneja, Parveen},
  Journal                  = {Science},
